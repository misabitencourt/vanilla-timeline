    "webpack": "2.1.0-beta.27"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.5",
    "babel-plugin-transform-es2015-template-literals": "^6.8.0",
    "cordova": "^6.5.0",
    "cordova-icon": "^0.9.1",
    "css-loader": "^0.26.2",
    "html-loader": "^0.4.5",
    "image-webpack-loader": "^3.2.0",
